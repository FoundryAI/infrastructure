[
  {
     "Name": "${name}",
     "Image": "${image}",
     "Essential": true,
     "Memory": "${memory}",
     "MemoryReservation": "${memory_reservation}",
     "LogConfiguration": {
        "LogDriver": "awslogs",
        "Options": {
          "awslogs-group": "${awslogs_group}",
          "awslogs-region": "${aws_region}",
          "awslogs-stream-prefix": "${environment}"
        }
     },
     "Environment": [
        {
           "Name": "Tag",
           "Value": "latest"
        },
        {
           "Name": "AWS_ACCOUNT_ID",
           "Value": "${aws_account_id}"
        },
        {
           "Name": "AWS_REGION",
           "Value": "${aws_region}"
        },
        {
           "Name": "AWS_ACCESS_KEY",
           "Value": "${aws_access_key}"
        },
        {
           "Name": "AWS_SECRET_KEY",
           "Value": "${aws_secret_key}"
        },
        {
           "Name": "ENVIRONMENT",
           "Value": "${environment}"
        }
     ]
  }
]
